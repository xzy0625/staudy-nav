    "react-dom": "17.x",
    "tailwindcss": "^3.2.7",
    "umi": "^3.5.36"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "^2.1.6",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.5.36",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
