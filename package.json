    "@cloudbase/cli": "^2.2.2",
    "@cloudbase/js-sdk": "1.5.0",
    "ahooks": "^2.0.0",
    "antd": "^4.24.7",
    "antd-img-crop": "^4.6.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.3",
    "dotenv": "^16.0.3",
    "dva": "2.4.1",
    "js-cookie": "2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "postcss": "^8.4.21",
    "querystring": "^0.2.1",
